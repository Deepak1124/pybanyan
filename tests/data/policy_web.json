{
  "kind": "BanyanPolicy",
  "apiVersion": "rbac.banyanops.com/v1",
  "metadata": {
    "name": "jupyter-allow",
    "description": "Jupyter users",
    "tags": {
      "template": "USER"
    }
  },
  "type": "USER",
  "spec": {
    "access": [
      {
        "roles": [
          "ANY"
        ],
        "rules": {
          "l7_access": [
            {
              "resources": [
                "*"
              ],
              "actions": [
                "*"
              ]
            }
          ],
          "conditions": {
            "trust_level": "Low"
          }
        }
      }
    ],
    "exception": {
      "src_addr": []
    },
    "options": {
      "disable_tls_client_authentication": true,
      "l7_protocol": "http"
    }
  }
}